ALGORITHM insertion_sort
VAR
    set  : ARRAY_OF INTEGER[10];
    i,j , temp : INTEGER
BEGIN
    FOR i FROM 0 TO set.lenght-1 STEP  1  DO
         FOR j = i+1 and j>0  STEP -1  DO
            IF (arr[j] < arr[j-1]) THEN
                tem := arr [j]
                arr[j] := arr[j-1]
                arr [j-1] := temp
            END_IF
         END_FOR

    END_FOR

END